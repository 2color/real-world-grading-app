  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^20.0.3",
    "joi": "^17.3.0",
    "@prisma/client": "2.13.1",
    "@sendgrid/mail": "^7.4.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "hapi-auth-jwt2": "^10.1.0",
    "hapi-pino": "^8.3.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.13.1",
    "@types/hapi-pino": "^8.0.1",
    "@types/hapi__hapi": "^20.0.3",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "compile": "tsc",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "seed": "ts-node ./src/seed.ts",
    "seed-users": "ts-node ./src/seed-users.ts",
    "test": "TEST=true jest",
    "test:watch": "TEST=true jest --watch",
    "postgres:start": "docker-compose up -d",
    "db:push": "prisma db push --preview-feature",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "prisma:generate": "prisma generate"
  },
  "engines": {
    "node": "14.x"
  }
}
