  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^20.0.1",
    "joi": "^17.3.0",
    "@prisma/client": "2.7.0",
    "@sendgrid/mail": "^7.3.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
